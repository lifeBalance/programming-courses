{
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },

  "globals": {
    "jQuery": true,
    "$": true
  },

  "ecmaFeatures": {
    "jsx": true
  },

  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-undescore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0
  }
}
